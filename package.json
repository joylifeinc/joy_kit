  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.js --env=example",
    "build": "webpack --debug --config ./webpack.config.js --env=build",
    "example-pages": "webpack --debug --config ./webpack.config.js --env=buildexample",
    "analyze": "webpack --debug --config ./webpack.config.js --env=analyze",
