{
  "name": "@withjoy/joykit",
  "version": "0.0.10",
  "description": "JoyKit UI Components",
  "main": "./lib/joykit.esm.js",
  "module": "./lib/joykit.esm.js",
  "types": "./lib/src/index.d.ts",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --config ./webpack.config.js --env=example",
    "build-watch": "webpack --debug --watch --config ./webpack.config.js --env=build",
    "build": "npm run clean && npm run rollup",
    "rollup": "rollup -w -c",
    "compile": "tsc --rootDir ./src --outDir ./dist",
    "clean": "npm run clean-dist && npm run clean-lib",
    "clean-dist": "shx rm -rf ./dist",
    "clean-lib": "shx rm -rf ./lib",
    "example-pages": "webpack --debug --config ./webpack.config.js --env=buildexample",
    "analyze": "webpack --debug --config ./webpack.config.js --env=analyze",
    "test": "node_modules/.bin/jest --forceExit --no-cache  --config jest.config.js",
    "lint": "tslint -c ./tslint.json -p ./tsconfig.json",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "prettier": "prettier --write --config .prettierrc \"src/**/*.{tsx,jsx,js}\"",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "postpublish": "npm run build",
    "build-storybook": "build-storybook -c .storybook -o docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joylifeinc/joy_kit.git"
  },
  "keywords": [
    "joykit"
  ],
  "author": "Joy Life Services <services@withjoy.com>",
  "license": "ISC",
  "homepage": "https://github.com/joylifeinc/joy_kit",
  "dependencies": {
    "@types/react": "^16.0.31",
    "debug": "^3.1.0",
    "glamor": "^2.20.40",
    "moment": "^2.19.3",
    "react": "^16.2.0",
    "react-add-to-calendar": "^0.1.5",
    "react-datepicker": "^0.61.0",
    "react-dom": "^16.0.0",
    "react-places-autocomplete": "^5.4.3",
    "react-portal": "^4.1.0",
    "rxjs": "^5.5.4",
    "velocity-animate": "^1.5.0",
    "velocity-react": "^1.3.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.16",
    "@storybook/addon-knobs": "^3.2.16",
    "@storybook/addon-links": "^3.2.16",
    "@storybook/addon-options": "^3.2.16",
    "@storybook/react": "^3.2.16",
    "@types/enzyme": "^3.1.5",
    "@types/jest": "^21.1.8",
    "@types/react": "^16.0.29",
    "@types/storybook__react": "^3.0.5",
    "awesome-typescript-loader": "^3.4.1",
    "clean-webpack-plugin": "^0.1.17",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-css-modules": "^1.1.0",
    "minimist": "^1.2.0",
    "prettier": "^1.8.2",
    "prism-themes": "^1.0.0",
    "prismjs": "^1.8.4",
    "react-hot-loader": "^3.1.3",
    "react-router-dom": "^4.2.2",
    "rollup": "^0.52.1",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-image": "^1.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript2": "^0.8.4",
    "rollup-plugin-url": "^1.3.0",
    "rollup-plugin-visualizer": "^0.3.1",
    "scss-loader": "0.0.1",
    "shx": "^0.2.2",
    "style-loader": "^0.19.0",
    "ts-jest": "^21.2.4",
    "ts-loader": "^3.1.1",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-closure-compiler": "^2.1.5",
    "webpack-dev-server": "^2.9.4"
  }
}
